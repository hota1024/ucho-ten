{
  "name": "bluesky-react",
  "description": "bluesky-react is an experimental Bluesky client library for React",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react --sourcemap",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts --external react --sourcemap",
    "lint": "eslint \"src/**/*.ts*\"",
    "test": "jest",
    "mock-server": "ts-node src/testing-utils/mock-server.ts --type module",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@atproto/dev-env": "^0.2.2",
    "@jest/globals": "^29.6.2",
    "@testing-library/jest-dom": "latest",
    "@testing-library/react": "latest",
    "@testing-library/react-hooks": "latest",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@ucho-ten/eslint-config-custom": "workspace:*",
    "@ucho-ten/tsconfig": "workspace:*",
    "cross-fetch": "^4.0.0",
    "eslint": "^8.15.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "react": "^18.1.0",
    "ts-jest": "^29.1.1",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@atproto/api": "^0.4.3",
    "zustand": "^4.3.9"
  }
}